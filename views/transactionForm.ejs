<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('./partials/metaHead'); %>
</head>
<body>
  <%- include('./partials/header'); %>
  <form action="" method="post" class="ui form">
    <label>Category Name:</label>
    <select name="year" class="ui dropdown">
    <% for(let [index,category] of categories.entries()) {%>
        <option value= "<%= index+1 %>" ><%=category.name%></option> 
    <%} %>
    </select>
    <label>Transaction Name:</label>
    <input type="text" name="name" value="<%= transaction.name || '' %>" placeholder="Name">
    <label>Price:</label>
    <input type="text" name="name" value="<%= transaction.price || '' %>" placeholder="Name">
    
    
    <input class="ui primary button" type="submit" value="Save">
  </form>
</body>
</html>
